{
  "name": "columnsgalore",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode production --target app --modern",
    "buildDev": "vue-cli-service build --mode development --watch",
    "lint": "vue-cli-service lint",
    "audit": "npm audit fix",
    "sitemap": "vue-cli-service sitemap",
    "format": "prettier --write \"**/*.{js,vue}\" && terraform fmt -recursive terraform",
    "postinstall": "cd srv && npm install"
  },
  "dependencies": {
    "core-js": "^3.15.2",
    "vue": "^2.6.14",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.5.2",
    "vuetify": "^2.5.6",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-router": "^4.3.1",
    "@vue/cli-plugin-vuex": "^4.3.1",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "compression-webpack-plugin": "^4.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "prerender-spa-plugin": "^3.2.1",
    "prettier": "^3.2.5",
    "sass": "^1.35.2",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-prerender-spa": "^1.1.6",
    "vue-cli-plugin-sitemap": "^2.1.1",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.2"
  },
  "eslintConfig": {
    "rules": {
      "no-console": "warn"
    },
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "engines": {
    "node": "^14.0.0"
  }
}
